{
  "name": "DecryptPg",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "DecryptPg",
  "homepage_url": "http://www.github.com/nvwo",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
  },
  "web_accessible_resources": ["index.html","inline.js"],
  "content_scripts": [{
    "matches": ["*://github.com/nvwo/RepoForTest/*","*://www.github.com/nvwo/RepoForTest/*"],
    "js": ["scripts.js"],
    "run_at": "document_end"
  }],
  "browser_action": {
    "default_title": "设置"
  },
  "permissions": [
    "https://github.com/*",
    "http://github.com/*"
  ],
  "content_security_policy":"script-src 'self';object-src 'self'"
}